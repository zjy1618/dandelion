<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.zjy.dandelion.dao.mapper.ProductSortMapper">
  <resultMap id="ListMap" type="com.zjy.dandelion.entity.ProductSort" >
  </resultMap>
  
  <resultMap id="DetailMap" type="com.zjy.dandelion.entity.ProductSort" extends="ListMap"></resultMap>
  
  <select id="selectByPrimaryKey" resultMap="DetailMap">
  	select * from `product_sort` where id=#{id}
  </select>
  
  <select id="findAll"  resultMap="ListMap">
  	select * from `product_sort` order by createDate desc;
  </select>
</mapper>
