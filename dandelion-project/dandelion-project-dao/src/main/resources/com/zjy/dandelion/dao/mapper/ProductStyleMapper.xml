<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.zjy.dandelion.dao.mapper.ProductStyleMapper">
  <resultMap id="ListMap" type="com.zjy.dandelion.entity.ProductStyle" >
  </resultMap>
  
  <resultMap id="DetailMap" type="com.zjy.dandelion.entity.ProductStyle" extends="ListMap"></resultMap>
  
  <select id="selectByPrimaryKey" resultMap="DetailMap">
  	select * from `product_style` where id=#{id}
  </select>

  <select id="findByProductCode" resultMap="ListMap">
    select * from `product_style` where productCode = #{productCode}
  </select>

</mapper>